{
  "name": "Schultility",
  "description": "Schulnetz Utility Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "/html/index.html",
    "default_icon": "/img/icon.png" 
  },
    "content_scripts": [
        {
            "js": ["js/popup.js"],
            "matches": [
                
                "https://*/*"
                
            ]
        }
    ]
}
